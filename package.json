{
  "name": "ethical_geo",
  "version": "1.0.0",
  "description": "The EthicalGEO Wordpress theme",
  "main": "index.js",
  "scripts": {
    "build": "rm style.css && npm run sass",
    "sass": "node-sass --output-style compressed style.scss > style.css",
    "watch:sass": "nodemon -e scss -x 'npm run sass'"
  },
